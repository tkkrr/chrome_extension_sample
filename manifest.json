{
    "manifest_version": 2,
    "name": "Text Input Buffer for Chrome",
    "version": "0.0.1",
    "content_scripts": [{
        "matches": ["http://www.google.com/*", "https://www.google.com/*"],
        "js": ["index.js"],
        "css": ["style.css"]
    }],
    "permissions": [
        "http://www.google.com/*"
    ]
}